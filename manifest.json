{
  "manifest_version": 3,
  "name": "RoleCase Job Importer",
  "version": "2.3",
  "description": "On-demand parsing for job applications.",
  "permissions": [
    "activeTab",
    "scripting",
    "storage",
    "cookies"
  ],
  "host_permissions": [
    "http://localhost:8000/*",
    "http://127.0.0.1:8000/*"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  }
}